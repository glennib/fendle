{
    "manifest_version": 2,
    "name": "Fendle",
    "version": "0.0.0",
    "description": "Legger til pendletider for boligannonser p√• Finn.no.",
    "icons": {
        "48": "icons/commute.svg",
        "96": "icons/commute.svg"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.finn.no/realestate/homes/ad.html*",
                "*://*.finn.no/realestate/homes/search.html*"
            ],
            "js": [
                "API_KEY",
                "scripts/finn_realestate.js"
            ]
        },
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "API_KEY",
                "scripts/all.js"
            ]
        }
    ],
    "background": {
        "scripts": [
            "background/auxiliary.js"
        ]
    }
}